{
  "name": "angular2-webpack",
  "version": "1.0.0",
  "description": "A webpack starter for angular 2",
  "keywords": [
    "Angular2",
    "Typescript",
    "webpack",
    "starter"
  ],
  "scripts": {
    "start": "webpack-dev-server --inline --progress --port 8080 --host 0.0.0.0",
    "test": "karma start",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail && npm run precache",
    "precache": "sw-precache --verbose --config=config/sw-precache-config.js"
  },
  "author": "Brandon Thompson github.com/creativeBrandon",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "4.0.0-rc.2",
    "@angular/compiler": "4.0.0-rc.2",
    "@angular/core": "4.0.0-rc.2",
    "@angular/forms": "4.0.0-rc.2",
    "@angular/http": "4.0.0-rc.2",
    "@angular/platform-browser": "4.0.0-rc.2",
    "@angular/platform-browser-dynamic": "4.0.0-rc.2",
    "@angular/router": ">=4.0.0-beta <5.0.0",
    "core-js": "^2.4.1",
    "material-design-lite": "^1.3.0",
    "rxjs": "5.2.0",
    "sanitize.css": "^5.0.0",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/googlemaps": "^3.25.37",
    "@types/jasmine": "^2.5.35",
    "@types/node": "^7.0.5",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.2",
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.0.8",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.5",
    "html-webpack-include-assets-plugin": "0.0.4",
    "html-webpack-plugin": "^2.28.0",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.5.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.13.2",
    "sw-precache": "^5.0.0",
    "to-string-loader": "^1.1.5",
    "typescript": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "~3.0.0"
  }
}
